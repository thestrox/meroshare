<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.the_strox.meroshareremade">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Mero Share"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BaseActivity"></activity>
        <activity android:name=".LiveActivity"></activity>
        <activity android:name=".ListedActivity"></activity>
        <activity android:name=".TestActivity"></activity>
        <activity android:name=".PortfolioActivity_main"></activity>
        <activity android:name=".PortfolioActivity_add"></activity>
        <activity android:name=".CalculatorActivity"></activity>
        <activity android:name=".PortfolioActivity_login"></activity>
        <activity android:name=".PortfolioActivity_changepassword"></activity>

    </application>

</manifest>